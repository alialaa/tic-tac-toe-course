{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "typecheck": "tsc",
        "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
        "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run typecheck && lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint",
        "**/*.{js,jsx,json,ts,tsx}": "prettier --write"
    },
    "dependencies": {
        "@expo-google-fonts/delius-unicase": "^0.1.0",
        "@react-native-async-storage/async-storage": "^1.14.1",
        "@react-native-community/async-storage": "^1.12.1",
        "@react-native-community/masked-view": "0.1.10",
        "@react-native-community/netinfo": "^6.0.0",
        "@react-navigation/native": "^5.9.2",
        "@react-navigation/stack": "^5.14.2",
        "@twotalltotems/react-native-otp-input": "^1.3.7",
        "aws-amplify": "^3.3.22",
        "aws-amplify-react-native": "^4.3.1",
        "expo": "~39.0.2",
        "expo-av": "~8.6.0",
        "expo-font": "~8.3.0",
        "expo-haptics": "~8.3.0",
        "expo-linear-gradient": "~8.3.0",
        "expo-status-bar": "~1.0.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.4.tar.gz",
        "react-native-gesture-handler": "~1.7.0",
        "react-native-reanimated": "~1.13.0",
        "react-native-safe-area-context": "3.1.4",
        "react-native-screens": "~2.10.1",
        "react-native-web": "~0.13.12",
        "react-navigation": "^1.0.0-beta.11"
    },
    "devDependencies": {
        "@babel/core": "~7.9.0",
        "@types/react": "~16.9.35",
        "@types/react-dom": "~16.9.8",
        "@types/react-native": "~0.63.2",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-react": "^7.21.5",
        "husky": "^4.3.4",
        "lint-staged": "^10.5.3",
        "prettier": "2.2.1",
        "typescript": "~3.9.5"
    },
    "private": true
}
